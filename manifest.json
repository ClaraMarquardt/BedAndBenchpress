{
  "manifest_version": 2,
  "name": "airbnb map",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.1.1.min.js","content.js"]
  }
],
  "permissions": [
    "tabs",
    "storage",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
  "scripts": ["background.js"]
},
//   "browser_action": {
//   "default_icon": "marker.png"
// }
"page_action": {
      "default_name": "Display Map",
      "default_icon": "marker.png",
      "default_popup": "popup.html"
  }
}